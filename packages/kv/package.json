{
  "name": "@compiel/kv",
  "description": "Key-value store utilities using Upstash Redis for Compiel",
  "version": "1.0.0",
  "dependencies": {
    "@upstash/redis": "^1.34.2",
    "server-only": "0.0.1"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**",
    "README.md"
  ],
  "keywords": [
    "cache",
    "compiel",
    "key-value",
    "redis",
    "upstash"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/compiel/comp.git",
    "directory": "packages/kv"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "clean": "rm -rf .turbo node_modules dist",
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "typecheck": "tsc --noEmit"
  },
  "types": "./dist/index.d.ts"
}
